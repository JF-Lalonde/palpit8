<h1>Admin</h1>
<table>
<% @games.each do |game| %>
  <tr>
  <td><%= game.name %></td>
  <td><%= link_to "Link", admin_game_path(game) %></td>
  <td><%= link_to "Edit", edit_admin_game_path(game) %></td>
  <td><%= link_to "Delete", admin_game_path(game), method: :delete %></td>
  </tr>
<% end %>
</table>
<p><%= link_to "New Game", new_admin_game_path %></p>
